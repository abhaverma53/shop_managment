<p style="color: green"><%= notice %></p>

<h1>Clients</h1>
<%= link_to "New client", new_client_path,class:"btn btn-primary" %>

<div id="clients">
  <% @clients.each do |client| %>
    <%= render client %>
    <p>
      <%= link_to "Show this client", client,class:"btn btn-warning" %>
    </p>
  <% end %>
</div>



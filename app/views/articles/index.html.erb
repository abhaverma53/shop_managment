<p style="color: green"><%= notice %></p>

<h1>Articles</h1>

<%= link_to "New Book", new_article_path, class:"btn btn-primary" %>

<div id="articles">
  <% @articles.each do |article| %>
    <%= render article %>
    <p>
      <%= link_to "Show this book", article, class:"btn btn-outline-info" %>
    </p>
  <% end %>
</div>
